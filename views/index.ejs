<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
  </head>
  <body>
    <h1><%= title %></h1>
    <p>Todos los videos en el servidor</p>
    <% allVids.forEach((video)=>{ %>
    <% console.log(video.videoURL) %>
    	<hr> 
    	<video controls="true" type='video/mp4' src="<%= "uploads"+ video.videoURL %>" poster="<%= "uploads"+video.videoURL.replace('mp4','png') %>" playsinline>
    	</video>
	<% });%>

  </body>
</html>
